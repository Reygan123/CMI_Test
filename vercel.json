{
    "version": 2,
    "builds": [
      {
        "src": "public/*",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "public"
        }
      }
    ],
    "routes": [
      {
        "handle": "filesystem"
      },
      {
        "src": "/.*",
        "dest": "/index.php"
      }
    ]
  }
  